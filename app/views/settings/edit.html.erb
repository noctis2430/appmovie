</div class="container">
	<%= form_for @setting do |f| %>
		<div class="field">
			<b>Pais:</b> <%= f.text_field :country %> <br>
		</div>
		<div class="field">
			<b>Idioma:</b> <%= f.text_field :language %> <br>
		</div>
		<%= f.submit "Guardar", class:"btn btn-sm btn-danger white", style:"margin-top: 15px;" %> <br /><br />

	<%end%>
</div>